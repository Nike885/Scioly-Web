{
  "expo": {
    "name": "Scioly App",
    "slug": "sciolyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#94cfec"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/keyclubapp"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/videos/*"
    ],
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parthzanwar.keyclubapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#94cfec"
      },
      "package": "com.parthzanwar.keyclubapp"
    },
    "web": {
      "favicon": "./assets/images/keyclublogo.png",
      "serviceWorker": false,
      "bundler": "webpack",
      "build": {
        "babel": {
          "include": ["@expo/vector-icons"]
        }
      }
    },
    "plugins": [
      "expo-video"
    ]
  }
}
